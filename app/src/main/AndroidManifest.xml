<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="id.smkcoding.smkcodingproject2">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/logo"
            android:label="@string/app_name"
            android:roundIcon="@drawable/logo"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".DetailProvinsiIndonesiaActivity">
        </activity>
        <activity android:name=".DetailProvinsiActivity">
        </activity>
        <activity android:name=".DetailNegaraNegaraActivity">
        </activity>
        <activity android:name=".ProvinsiActivity">
        </activity>
        <activity android:name=".IndonesiaActivity">
        </activity>
        <activity android:name=".DetailNegaraActivity">
        </activity>
        <activity android:name=".GlobalActivity">
        </activity>
        <activity android:name=".RegisterActivity">
        </activity>
        <activity android:name=".HomeActivity">
        </activity>
        <activity android:name=".LoginActivity">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <meta-data
                android:name="preloaded_fonts"
                android:resource="@array/preloaded_fonts"/>
    </application>

</manifest>